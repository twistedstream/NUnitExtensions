    <Input_Company>Twisted Stream Technologies, Inc.</Input_Company>
    <Input_Product>NUnitExtensions</Input_Product>
    <Input_VersionMajor>1</Input_VersionMajor>
    <Input_VersionMinor>0</Input_VersionMinor>
    <Input_VersionRevision>1</Input_VersionRevision>
    <Input_VersionInformation>RC</Input_VersionInformation>
  </PropertyGroup>
  
